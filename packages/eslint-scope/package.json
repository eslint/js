  },
  "devDependencies": {
    "@typescript-eslint/parser": "^8.7.0",
    "chai": "^6.0.0",
    "eslint-visitor-keys": "^5.0.0",
    "espree": "^11.0.0",
    "npm-license": "^0.3.3",

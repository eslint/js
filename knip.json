{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "mocha": {
    "entry": ["tests/**/*.{cjs,cts,js,ts}"]
  },
  "workspaces": {
    "packages/eslint-visitor-keys": {
      "entry": ["test-d/**/*.test-d.ts"]
    },
    "packages/espree": {
      "entry": ["espree.cts", "test-d/**/*.test-d.ts", "tools/**/*.js"],
      "ignore": ["tests/fixtures/**"]
    }
  }
}
